msgid "# Импорт необходимых инструментов\n"
"import os\n"
"import pandas as pd\n"
"\n"
"# Импорт модуля\n"
"from oceanai.modules.lab.build import Run\n"
"\n"
"# Создание экземпляра класса\n"
